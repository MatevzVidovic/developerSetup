# Zen MCP Server Configuration
# Copy this file to .env and fill in your API keys

# For consulting various AI models
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# For free Gemini model consultation
GOOGLE_API_KEY=your_google_api_key_here

# Model preferences
DEFAULT_MODEL=gemini-1.5-flash  # Use free Gemini model by default
FALLBACK_MODEL=gpt-3.5-turbo    # Fallback to cheaper OpenAI model

# Server configuration
PORT=3000
LOG_LEVEL=info
